
 <table border="1" class="content-table table-hover" style="width:100%">
  <thead>
  <tr>
    <th>Index</th>
    <th >Notifications</th>
  </tr>
  </thead>
  <tbody>
  {{#each obj}}
    {{#if_eq this.Record.Name "AssetTransfer"}}
    {{#if_eq this.Record.NewOwnerCNIC ../NID}}
    <tr>
      <td></td>
    <td align="center">AssetCode:<b>{{this.Record.AssetCode}}</b><br/>PreviousOwnerCNIC:<b>{{this.Record.PreviousOwnerCNIC}}</b><br/>
    PreviousOwnerName:<b>{{this.Record.PreviousOwnerName}}</b>
     <br/>has been transferred to you at {{this.Record.Time}}
     <br/> please view your asset and apply for deed Registration.
     </td> 
    </tr>
    {{/if_eq}}
    {{#if_eq this.Record.PreviousOwnerCNIC ../NID}}
    <tr>
      <td></td>
    <td align="center">AssetCode:<b>{{this.Record.AssetCode}}</b><br/>has been transferred to<br/>
    CNIC:<b>{{this.Record.NewOwnerCNIC}}</b> Name:<b>{{this.Record.NewOwnerName}}</b><br/>
    at {{this.Record.Time}}
    </td>
    </tr>
    {{/if_eq}}

    {{#if_eq this.Record.OwnerNID ../NID}}
    <tr>
      <td></td>
    <td align="center">Request for your  AssetCode:<b>{{this.Record.AssetCode}}</b><br/>by CNIC:<b>{{this.Record.RequestedByNID}}</b><br/>has been rejected by The Registrar
    at {{this.Record.Time}}</td>
    </tr>
    {{/if_eq}}
    {{#if_eq this.Record.RequestedByNID ../NID}}
    <tr>
      <td></td>
    <td align="center">Your Request for AssetCode<b>:{{this.Record.AssetCode}}</b><br/>Owned By CNIC:<b>{{this.Record.OwnerNID}}</b><br/>has been rejected by The Registrar
    at {{this.Record.Time}}
    </td>
    </tr>
    {{/if_eq}}
    {{/if_eq}}
  {{/each}}
  </tbody>
</table> 

{{!-- {{result}} --}}
