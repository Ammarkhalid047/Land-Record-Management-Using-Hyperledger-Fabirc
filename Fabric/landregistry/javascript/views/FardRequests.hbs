
 {{#if_eq success "accepted"}}
    <div class="alert alert-success">
    <ul>
        <li>Fard has been sent to the portal</li>
    </ul>
</div>
{{/if_eq}} 
 {{#if_eq success "rejected"}}
 <div class="alert alert-success">
    <ul>
        <li>Request Rejected</li>
    </ul>
</div>
{{/if_eq}}

{{!-- class="table table-hover row-clickable" --}}
 <table  border="1" class="content-table table-hover" style="width:100%; margin-top: 25px;">
  <thead>
  <tr>
    <th>Index</th>
    <th>Request Code</th>
    <th>AssetName</th>
    <th>OwnerCNIC</th>
    <th>RequestedBy(CNIC)</th>
    <th>Time</th>
  </tr>
  </thead>
  <tbody>
  {{#each obj}}
    {{#if_eq this.Record.OwnerAck "1"}}
    {{#if_eq this.Record.RequestType "1"}}
    <tr>
      <td></td>
    <td align="center"><a style="text-decoration:none;color:white;" href="{{joinPath '/admin/ViewRequest/' this.Key}}">{{this.Key}}</a></td>
    <td align="center"><a style="text-decoration:none;color:white;" href="{{joinPath '/admin/ViewRequest/' this.Key}}">{{this.Record.AssetName}}</a></td>
    <td align="center"><a style="text-decoration:none;color:white;" href="{{joinPath '/admin/ViewRequest/' this.Key}}">{{this.Record.OwnerCNIC}}</a></td>
    <td align="center"><a style="text-decoration:none;color:white;" href="{{joinPath '/admin/ViewRequest/' this.Key}}">{{this.Record.RequestedByNID}}</a></td>
    <td align="center"><a style="text-decoration:none;color:white;" href="{{joinPath '/admin/ViewRequest/' this.Key}}">{{this.Record.Time}}</a></td>
    </tr>
    {{/if_eq}}
    {{/if_eq}}
  {{/each}}
  </tbody>
</table> 

