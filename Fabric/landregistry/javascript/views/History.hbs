    <td align="center">{{obj.Khata_No}}</td>
  </tr>
  <tr>
    <td align="center">Khasra Number :</td>
    <td align="center">{{obj.Khasra_No}}</td>
  </tr>
  <tr>
    <td align="center">Family Number :</td>
