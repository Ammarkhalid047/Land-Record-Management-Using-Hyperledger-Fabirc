<h1>History:</h1>
<div class="vertical-menu data-tables">
<table border="0" class="table row-clickable" style="width:100%">
  
  {{#each obj1}}
  <tr>
    <td  align="center">TransactionId: {{this.TxId}}<br/>TransactionTime: {{this.Timestamp}}<br/>
    {{#if_eq this.IsDelete "false"}}
    AssetName: {{this.Value.AssetName}}<br/>OwnerCNIC: <b>{{this.Value.OwnerCNIC}}</b><br/>OwnerName: {{this.Value.OwnerName}}<br/>
    Advertised: {{this.Value.Advertised}}
    </td>
    {{else}}
     Asset has been deleted at this point
    {{/if_eq}}
  </tr>
  {{/each}}
</table> 
  </div>




