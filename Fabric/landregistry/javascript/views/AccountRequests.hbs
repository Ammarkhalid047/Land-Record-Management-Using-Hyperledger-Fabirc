
 {{#if_eq success "accepted"}}
    <div class="alert alert-success">
    <ul>
        <li>Account has been approved</li>
    </ul>
</div>
{{/if_eq}} 
 {{#if_eq success "rejected"}}
 <div class="alert alert-success">
    <ul>
        <li>Account Rejected</li>
    </ul>
</div>
{{/if_eq}}

{{!-- class="table table-hover row-clickable" --}}
 <table  border="1" class="content-table table-hover" style="width:100%; margin-top: 25px;">
  <thead>
  <tr>
    <th>Index</th>
    <th>CNIC</th>
    <th>Name</th>
    <th>Email</th>
    <th>Mobile</th>
  </tr>
  </thead>
  <tbody>
  {{#each obj}}
    {{#if_eq this.Record.Verified "No"}}
    <tr>
      <td></td>
    <td align="center"><a style="text-decoration:none;color:white;" href="{{joinPath '/admin/ViewAccountRequests/' this.Key}}">{{this.Key}}</a></td>
    <td align="center"><a style="text-decoration:none;color:white;" href="{{joinPath '/admin/ViewAccountRequests/' this.Key}}">{{this.Record.Name}}</a></td>
    <td align="center"><a style="text-decoration:none;color:white;" href="{{joinPath '/admin/ViewAccountRequests/' this.Key}}">{{this.Record.Email}}</a></td>
    <td align="center"><a style="text-decoration:none;color:white;" href="{{joinPath '/admin/ViewAccountRequests/' this.Key}}">{{this.Record.Mobile}}</a></td>
    </tr>
    {{/if_eq}}
  {{/each}}
  </tbody>
</table> 

